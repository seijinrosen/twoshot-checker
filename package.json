    "@types/react-dom": "18.0.11",
    "@vitejs/plugin-react": "3.0.0",
    "cspell": "^6.18.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "typescript": "4.9.4",
