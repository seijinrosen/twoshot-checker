    "@types/react-dom": "18.0.6",
    "@vitejs/plugin-react": "2.1.0",
    "cspell": "^6.18.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
